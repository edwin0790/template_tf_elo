@Thesis{aguadopd2017,
  author      = {Aguado, Pablo Daniel and Secchi, Humberto and Orellana, Adrián and Rossomando, Francisco and Zavalla, Eduardo},
  title       = {Una estrategia para la clasificación óptica de almendras},
  institution = {Universidad Nacional de San Juan},
  year        = {2017},
  language    = {spanish},
  location    = {San Juan, Argentina},
  abstract    = {En este trabajo se diseñó y creó un programa informático para la clasificación automática de almendras peladas a partir de imágenes de ellas, analizando diversas características de forma y de color. Para probarlo se construyó un prototipo de sistema de visión artificial con el cual se creó un conjunto de \num{564} imágenes de almendras y otros objetos. El conjunto de imágenes fue etiquetado manualmente en base a las normas de la Comisión Económica de las Naciones Unidas para Europa (UNECE) para el comercio de almendras. Los resultados del clasificador desarrollado son similares a los obtenidos con algoritmos de clasificación estándar, como máquinas de soporte vectorial o \ingles{Boosted Trees}. Los descriptores elegidos permiten clasificar binariamente el conjunto con una exactitud global de 93 \%.},
}

